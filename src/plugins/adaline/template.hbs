<div style="width:100%;text-align:center;">
  <div id="{{id}}" style="width:80%;height:0;padding-bottom:60%;margin:auto;"></div>
  <p>{{title}}</p>
</div>
<script type="text/javascript">
  var option = {
    xAxis: {
        type: '{{xtype}}',
        boundaryGap: false,
        data: adadata_{{datasetname}}.{{xdata}}
    },
    yAxis: {
        type: '{{ytype}}'
    },
    series: [
{{#each ydata}}
    {
        name: '{{{this.name}}}',
        data: adadata_{{../datasetname}}.{{{this.data}}},
        type: 'line',
        areaStyle: {}
    },
{{/each}}    
    ]
  };

  var chart_{{ id }} = adarender.initChart(document.getElementById('{{id}}'), option);
</script>